<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_hovered="false">
        <shape android:shape="oval">
            <corners android:radius="@dimen/parking_spot_circle_radius"/>
            <solid android:color="@color/japanese_laurel"/>
            <stroke android:color="@color/black" android:width="@dimen/stroke_tiny"/>
        </shape>
    </item>

    <item android:state_hovered="true">
        <shape android:shape="oval">
            <corners android:radius="@dimen/parking_spot_circle_radius"/>
            <solid android:color="@color/pomegranate"/>
            <stroke android:color="@color/black" android:width="@dimen/stroke_tiny"/>
        </shape>
    </item>
</selector>
